    <section id="inner-headline">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="breadcrumb">
                        <li><a href="#"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
                        <li class="active">Users</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section id="content">
    <div class="container">
		<div class="row">
			<div class="col-lg-8">
                <table class="table">
					<thead>
						<tr>
							<th>Name</th>
							<th>Email</th>
							<th>Affiliation</th>
                            <th>Message</th>
						</tr>
					</thead>
					<tbody>
                    <% for(var i = 0; i < rows.info.numRows; i ++ ) { %>
                        <tr>
                            <th><%=rows[i].name %></th>
                            <th><%=rows[i].email %></th>
                            <th><%=rows[i].affiliation %></th>
                            <th><%=rows[i].messages %></th>
                        </tr>
                        <%}%>
					</tbody>

				</table>
				<div id="pagination">
					<span class="all">Page 1 of 3</span>
					<span class="current">1</span>
					<a href="#" class="inactive">2</a>
					<a href="#" class="inactive">3</a>
				</div>
			</div>
			<div class="col-lg-4">
				<aside class="right-sidebar">
				<div class="widget">
					<h5 class="widgetheading">Search User</h5>
					<form class="form-search">
                        <div class="input-group">
                            <input class="form-control" type="text" placeholder="Search..">
                            <span class="input-group-btn">
                                <button type="submit" class="btn btn-default btn-large" data-original-title="" title="">Search</button>
                            </span>
                        </div>
					</form>
				</div>
				<div class="widget">
					<h5 class="widgetheading">Utilities</h5>
					<ul class="cat">
						<li><i class="icon-angle-right"></i><a href="#">Save as vCard</a></li>
						<li><i class="icon-angle-right"></i><a href="#">Save as Excel</a></li>
					</ul>
				</div>
				</aside>
			</div>
		</div>
	</div>
    </section>
